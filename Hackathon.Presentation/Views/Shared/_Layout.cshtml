<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Charlotte Hackathon - Team Dubmen</title>
        <link href="@Url.Content("~/Content/css/bootstrap.min.css")" rel="stylesheet" media="screen">
        <link href="@Url.Content("~/Content/css/select2.css")" rel="stylesheet">
        <link href="@Url.Content("~/Content/css/bootstrap-tag.css")" rel="stylesheet">
        <link href="@Url.Content("~/Styles/hackathon.css")" rel="stylesheet">
        @RenderSection("styles", required: false)
        <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-2.0.0.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Scripts/bootstrap.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Scripts/d3.v3.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Scripts/bootstrap-tag.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Scripts/spin.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Scripts/hackathon.js")"></script>
        @RenderSection("scripts", required: false)
    </head>
    <body>
        <div class="navbar navbar-inverse navbar-fixed-top">
          <div class="navbar-inner">
            <div class="container">
              <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <a class="brand" href="@Url.Content("~")">Dubmen Data Designs</a>
              <div class="nav-collapse collapse">
                <ul class="nav">
                  <li><a href="@Url.Content("~")">Home</a></li>
                  <li><a href="@Url.Content("~/SalesPerHousehold")">Sales Per Household</a></li>
                  <li><a href="@Url.Content("~/PurchasesByTime")">Purchases By Time</a></li>
                </ul>
              </div><!--/.nav-collapse -->
            </div>
          </div>
        </div>
        <div class="row-fluid">
            <div class="span12">
                <div class="container">
                @RenderBody()
                <p>
                    <!--[if lte IE 8]><span style="filter: FlipH; -ms-filter: "FlipH"; display: inline-block;"><![endif]-->
                    <span style="-moz-transform: scaleX(-1); -o-transform: scaleX(-1); -webkit-transform: scaleX(-1); transform: scaleX(-1); display: inline-block;">
                        &copy;
                    </span>
                    <!--[if lte IE 8]></span><![endif]--> 
                    @DateTime.Now.Year Dubmen Data Designs
                </p>
            </div>
        </div>
    </body>
</html>


